# eureka service app props
server:
    port: 8761

# The settings registerWithEureka and fetchRegistry are set to false,
# meaning that this Server is not part of a cluster.
eureka:
    client:
        register-with-eureka: false
        fetch-registry: false


logging:
  file: logs/Landscape-Eureka-Server.log    
  pattern:
    file: '%d{dd-MM-yyyy HH:mm:ss.SSS} [%thread] %-5level %logger{36}.%M - %msg%n'
  level:
    org.springframework.web: INFO
    com.avaya.ept.landscape.config: DEBUG
